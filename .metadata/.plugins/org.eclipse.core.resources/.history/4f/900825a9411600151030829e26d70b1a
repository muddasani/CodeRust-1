
public class reverseAlinkedList {
	
	public class LinkedListNode {
		int data;
		LinkedListNode next;
	}
	
	
	public static void reverseLinkedList(LinkedListNode head){
		
		LinkedListNode temp;
		LinkedListNode prev;
		
		while(head != null){
			temp = head;
			prev = head.next;
			head.next = temp;
			
			
		}
		
	}

}
